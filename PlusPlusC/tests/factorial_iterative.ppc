program Main;

func factorial(A int) -> int {
    if A == 0 {
        return 1;
    } 
    else {
        var result int;
        result = 1;
        while 1 < A {
            result = result * A;
            A = A - 1;
        }
        return result;
    }
}

main () {
    var i int;
    input(i);
    var fact int;

    fact = factorial(i);
    print(fact);
}